include block_html/_style.pug
include block_html/_include.pug

+header()
.page
	.page-cont
		+slideMenu()
		.content
			.section.section--mid.section--extend(style="background-image:url('img/bg-words.jpg')")
				.filter-bg.filter--dark
				.section-cont.section-cont--main
					.main-cont
						.section-title-line
						//.section-title-sub Lorem ipsum dolor sit amet
						.section-title Наши клиенты

			.section.section--white.section--extend-md
				.main-cont
					.client(data-aos="fade-down" data-aos-duration="1000")
						- for (var i=0;i<8;i++)
							.client__el
								img(src="img/client-1.png", alt="")
							.client__el
								img(src="img/client-2.png", alt="")
							.client__el
								img(src="img/client-3.png", alt="")
							.client__el
								img(src="img/client-4.png", alt="")
							.client__el
								img(src="img/client-5.png", alt="")


			+footer()
include ./block_html/_script.pug
